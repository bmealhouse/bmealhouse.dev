{
  "name": "bmealhouse.dev",
  "version": "1.0.0",
  "private": true,
  "lint-staged": {
    "*.{js,ts,tsx}": "xo"
  },
  "prettier": {
    "useTabs": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "bracketSpacing": true
  },
  "xo": {
    "prettier": true,
    "rules": {
      "capitalized-comments": "off"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install"
  },
  "dependencies": {
    "autoprefixer": "10.2.5",
    "date-fns": "2.19.0",
    "next": "10.0.8",
    "postcss": "8.2.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "2.0.3"
  },
  "devDependencies": {
    "@types/node": "14.14.34",
    "@types/react": "17.0.3",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "typescript": "4.2.3",
    "xo": "0.38.2"
  }
}
